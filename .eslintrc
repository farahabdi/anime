{
    "extends": [
      "airbnb"
    ],
    "globals": {
      "document": true
    },
    "rules": {
      "react/react-in-jsx-scope": ["off"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-use-before-define": ["error", { "functions": false, "classes": false }],
      "global-require": "off",
      "import/no-dynamic-require": "off"
    }
  }